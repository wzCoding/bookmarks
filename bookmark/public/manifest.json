{
    "manifest_version": 3,
    "name": "bookmark",
    "description": "a simple chrome bookmark extension",
    "version": "0.0.1",
    "background": {
        "service_worker": "./background/background.js"
    },
    "permissions": [
        "sidePanel",
        "bookmarks",
        "tabs",
        "favicon"
    ],
    "side_panel": {
        "default_path": "index.html"
    }
}